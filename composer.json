{
    "name": "mrjokermr/dynamic-modal-wrapper",
    "description": "Scaffholding for laravel & livewire modals",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mrjokermr\\DynamicModalWrapper\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Melle R."
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Mrjokermr\\DynamicModalWrapper\\DynamicModalWrapperServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^10.0 || ^11.0",
        "livewire/livewire": "^3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
